/*!40000 ALTER TABLE `datasources` ENABLE KEYS */;
UNLOCK TABLES;

--
-- Table structure for table `detections`
--

DROP TABLE IF EXISTS `detections`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `detections` (
  `technique_id` varchar(255) COLLATE utf8mb4_unicode_ci NOT NULL,
  `datasource_id` varchar(255) COLLATE utf8mb4_unicode_ci NOT NULL,
  `detection_en` varchar(255) COLLATE utf8mb4_unicode_ci NOT NULL,
  `detection_jp` varchar(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  PRIMARY KEY (`technique_id`,`datasource_id`),
  KEY `detections_technique_id` (`technique_id`),
  KEY `detections_datasource_id` (`datasource_id`),
  CONSTRAINT `detections_ibfk_1` FOREIGN KEY (`technique_id`) REFERENCES `techniques` (`technique_id`),
  CONSTRAINT `detections_ibfk_2` FOREIGN KEY (`datasource_id`) REFERENCES `datasources` (`datasource_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
/*!40101 SET character_set_client = @saved_cs_client */;

--
-- Dumping data for table `detections`
--

LOCK TABLES `detections` WRITE;
/*!40000 ALTER TABLE `detections` DISABLE KEYS */;
